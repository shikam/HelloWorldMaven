pipeline { 
    agent any 
    tools {
        tool name: 'Maven', type: 'maven'
    }
    stages {
            stage('Test'){
            steps { 
                        sh "mvn test"
            }
        }
        stage('Build') { 
            steps {
                        sh "mvn clean compile"
            }
        }
    }
    post { 
        always { 
            cleanWs()
        }
    }
}


